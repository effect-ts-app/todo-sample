        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "assignedTo": {
                    "type": "number",
                    "title": "PositiveInt",
                    "minimum": 0,
                    "nullable": true
                  },
                  "completed": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                  },
                  "due": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                  },
                  "isFavorite": {
                    "type": "boolean"
                  },
                  "myDay": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                  },
                  "note": {
                    "type": "string",
                    "title": "NonEmptyString",
                    "minLength": 1,
                    "nullable": true
                  },
                  "reminder": {
                    "type": "string",
                    "format": "date-time",
                    "nullable": true
                  },
