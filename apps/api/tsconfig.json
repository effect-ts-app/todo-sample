{
    "extends": "../../tsconfig.base.json",
    "compilerOptions": {
        "lib": [
            "dom",
            "esnext"
        ],
        "baseUrl": "./",
        "paths": {
            "@effect-ts-app/core/*": [
                "../../packages/core/*"
            ],
            "@effect-ts-app/infra/*": [
                "../../packages/infra/*"
            ],
            "@effect-ts-demo/todo-types/*": [
                "../../packages/types/*"
            ],
            "@effect-ts-demo/todo-client/*": [
                "../../packages/client/*"
            ],
            "@/*": [
                "./*"
            ]
        },
        "outDir": "./"
    },
    "include": [
        "./"
    ],
    "exclude": [
        "node_modules",
        "../../node_modules"
    ]
}