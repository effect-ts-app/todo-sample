{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "lib": [
            "dom",
            "esnext"
        ],
        "baseUrl": "./",
        "paths": {
            "@effect-ts-app/core/*": [
                "../../packages/core/*"
            ],
            "@effect-ts-app/core": [
                "../../packages/core"
            ],
            "@effect-ts-app/infra/*": [
                "../../packages/infra/*"
            ],
            "@effect-ts-app/infra": [
                "../../packages/infra"
            ],
            "@effect-ts-demo/todo-client/*": [
                "../../packages/client/*"
            ],
            "@effect-ts-demo/todo-client": [
                "../../packages/client"
            ],
            "@effect-ts-demo/todo-types/*": [
                "../../packages/types/*"
            ],
            "@effect-ts-demo/todo-types": [
                "../../packages/types"
            ],
            "@/*": [
                "./*"
            ]
        },
        "outDir": "./"
    },
    "include": [
        "./"
    ]
}