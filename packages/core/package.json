{
    "name": "@effect-ts-app/core",
    "version": "0.14.4",
    "main": "./index.js",
    "license": "MIT",
    "dependencies": {
        "@effect-ts/core": "^0.40.0",
        "@effect-ts/monocle": "^0.33.0",
        "@effect-ts/schema": "^0.8.1",
        "abort-controller": "^3.0.0",
        "path-parser": "^6.1.0",
        "short-uuid": "^4.2.0",
        "uuid": "^8.3.2",
        "validator": "^13.6.0"
    },
    "devDependencies": {
        "@types/validator": "^13.1.3",
        "node-libcurl": "^2.3.3",
        "ts-toolbelt": "^9.6.0"
    },
    "scripts": {
        "build": "yarn clean && tsc",
        "clean": "find . | egrep '\\.js(\\.map)?$|\\.d.ts$' | grep -v .eslint | grep -v jest.config | xargs rm -f",
        "postpublish": "yarn clean",
        "prepare": "yarn build",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
        "compile": "tsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "echo 'no tests yet'",
        "testsuite": "yarn compile && yarn lint && yarn test && yarn circular"
    },
    "publishConfig": {
        "access": "public"
    }
}
