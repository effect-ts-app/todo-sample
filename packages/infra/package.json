{
    "name": "@effect-ts-app/infra",
    "version": "0.9.3",
    "main": "index.js",
    "license": "MIT",
    "dependencies": {
        "@effect-ts-app/core": "^0.9.3",
        "mongodb": "^3.6.7",
        "proper-lockfile": "^4.1.2",
        "query-string": "^7.0.0",
        "redis": "^3.1.2",
        "redlock": "^4.2.0",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/mongodb": "^3.6.13",
        "@types/proper-lockfile": "^4.1.1",
        "@types/redis": "^2.8.28",
        "@types/redlock": "^4.0.1",
        "@types/winston": "^2.4.4"
    },
    "scripts": {
        "build": "yarn clean && tsc",
        "clean": "find . | egrep '\\.js$|\\.d.ts$' | grep -v .eslint | xargs rm -f",
        "postpublish": "yarn clean",
        "prepare": "yarn build",
        "circular": "madge --circular --ts-config ./tsconfig.json --extensions ts ./",
        "compile": "tsc --noEmit",
        "lint": "eslint .",
        "prettier": "prettier \"**/*.ts\"",
        "autofix": "yarn lint --fix && yarn prettier --write",
        "test": "echo 'no tests yet'",
        "testsuite": "yarn compile && yarn lint && yarn test && yarn circular"
    },
    "publishConfig": {
        "access": "public"
    }
}
