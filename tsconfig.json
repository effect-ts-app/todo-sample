{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@effect-ts-app/core/*": [
        "libs/packages/core/*"
      ],
      "@effect-ts-app/core": [
        "libs/packages/core"
      ],
      "@effect-ts-app/react/*": [
        "libs/packages/react/*"
      ],
      "@effect-ts-app/react": [
        "libs/packages/react"
      ],
      "@effect-ts-app/infra/*": [
        "libs/packages/infra/*"
      ],
      "@effect-ts-app/infra": [
        "libs/packages/infra"
      ],
      "@effect-ts-demo/todo-client/*": [
        "packages/client/*"
      ],
      "@effect-ts-demo/todo-client": [
        "packages/client"
      ],
      "@effect-ts-demo/todo-types/*": [
        "packages/types/*"
      ],
      "@effect-ts-demo/todo-types": [
        "packages/types"
      ],
      "@/*": [
        "apps/api/*"
      ],
      "@": [
        "apps/api"
      ]
    },
    "target": "ES2018",
    "module": "ESNext"
  }
}